<!DOCTYPE html>
<html>
  <head>
      <meta charset="utf-8">
    <title>{{ title }}</title>
    
    {{#stylesheets}}
    <link rel='stylesheet' href={{.}} />
    {{/stylesheets}}

      <script type="application/javascript" charset="UTF-8" src="http://d3js.org/d3.v3.min.js"></script>
  </head>
  <body>
    <div id="helperWindow"></div>
    {{#titleBar}}
    <div class={{style.divClasses}} id="titlebar">
        <span class="works" id="openFilter"><img src={{source.titleImage}}></span>
        <h1 class="offWhiteText sailregular">
        
        {{info.headerText}}
        
        </h1>
        
    </div>
    {{/titleBar}}
    
    {{#filterNavigator}}
    <div class={{style.divClasses}} id="filterNavigator">
        <h1 class="offWhiteText">{{info.headerText}}</h1>
        <div id="layerSelect">
            <h2 class="layerSelectHeader">Layers</h2>
            <ul>
            {{#layers}}
                <li id={{id}}>
                    <span>{{description}}</span>
                </li>
            {{/layers}}
            </ul>
        </div>
        
       
   
    </div>
     {{/filterNavigator}}
    <div id="mapDiv">
    
        {{#mapComponents}}
            <div id={{.}}></div>
        {{/mapComponents}}
        
    </div>
    <h1 id="legendHeader">Legend</h1>
    <div data-dojo-type="dijit/layout/ContentPane" id="legendPane"
           data-dojo-props="title:'Legend', selected:true">
    
    <div id="legendDiv">
        
    </div>
    </div>
    {{#reports}}
    <div class="brown" id="report">
        <h1 id="reportTitle">{{info.headerText}}</h1>
        <div class="offWhite" id="reportTop">
        <table id="reportTable">
            <tr>
                <td class="leftCol firstSection">
                    <div  id="statistics">
                        <h1 class="reportSectionHeader">{{info.statisticsHeader}}</h1>
                    </div>
                </td>
                <td class="rightCol" rowspan="2">
                    <div  id="charts">
                        <h1 class="reportSectionHeader">{{info.chartHeader}}</h1>
                        <div id="monthlyHighLow"></div>
                        <div id="weatherPrecip"></div>
                    </div>
                </td>
            </tr>
            <tr>
                <td  class="leftCol">
                    <div id="suggestions">
                    <h1 class="reportSectionHeader">{{info.suggestionHeader}}</h1>
                    </div>
                </td>
            </tr>
            
            
        </table>
        </div>
        
        
    </div>
    {{/reports}}

    <!--<div id="weatherForecast"></div>-->

    <script>var dojoConfig = { parseOnLoad:true };</script>
    {{#scripts}}
    <script src={{.}}></script>
    {{/scripts}}
    <script>
    
    
    </script>
    
    
  </body>
</html>